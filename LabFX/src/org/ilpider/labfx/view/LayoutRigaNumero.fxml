<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.ilpider.labfx.view.ControllerLayoutRigaNumero">
   <children>
      <HBox alignment="CENTER" spacing="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Button fx:id="btnPreso" maxHeight="30.0" maxWidth="35.0" minHeight="30.0" minWidth="30.0" mnemonicParsing="false" onAction="#doBtnPreso" />
            <CheckBox fx:id="chkA" disable="true" mnemonicParsing="false" />
            <CheckBox fx:id="chkB" disable="true" mnemonicParsing="false" />
            <CheckBox fx:id="chkC" disable="true" mnemonicParsing="false" />
            <Button fx:id="btnTogli" maxHeight="20.0" maxWidth="20.0" minHeight="20.0" minWidth="20.0" mnemonicParsing="false" onAction="#doBtnTogli" prefHeight="20.0" prefWidth="20.0" />
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </HBox>
   </children>
</AnchorPane>
